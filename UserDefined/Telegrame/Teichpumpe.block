var Virt_Schalter_Teichpumpe;


// Verhindern von gleichen Mitteilungen
Virt_Schalter_Teichpumpe = false;

on({id: 'hm-rpc.1.JEQ0738373.1.STATE', change: "ne"}, function (obj) {
  var value = obj.state.val;
  var oldValue = obj.oldState.val;
  if (getState("hm-rpc.1.JEQ0738373.1.STATE").val == false && Virt_Schalter_Teichpumpe == false) {
    sendTo("telegram.0", "send", {
        text: 'Achtung: Teichpumpe wurde ausgeschaltet',
        user: 'Schecki_Homematic, Ute, Dirk'
    });
    console.debug("telegram['Schecki_Homematic, Ute, Dirk']: " + 'Achtung: Teichpumpe wurde ausgeschaltet');
    Virt_Schalter_Teichpumpe = true;
  } else if (getState("hm-rpc.1.JEQ0738373.1.STATE").val == true && Virt_Schalter_Teichpumpe == true) {
    sendTo("telegram.0", "send", {
        text: 'Entwarnung: Teichpumpe wurde eingeschaltet',
        user: 'Schecki_Homematic, Ute, Dirk'
    });
    console.debug("telegram['Schecki_Homematic, Ute, Dirk']: " + 'Entwarnung: Teichpumpe wurde eingeschaltet');
    Virt_Schalter_Teichpumpe = false;
  }
});

