var Virt_Schalter_Teichbewegung;


// Verhindern von gleichen Mitteilungen
Virt_Schalter_Teichbewegung = false;

on({id: 'sonoff.0.NodeMcu_Teich.POWER1', change: "ne"}, function (obj) {
  var value = obj.state.val;
  var oldValue = obj.oldState.val;
  if ((obj.state ? obj.state.val : "") == true) {
    sendTo("telegram.1", "send", {
        text: 'Bewegung erkannt',
        user: ''
    });
    console.debug("telegram['']: " + 'Bewegung erkannt');
    Virt_Schalter_Teichbewegung = true;
  } else if ((obj.state ? obj.state.val : "") != true) {
    sendTo("telegram.1", "send", {
        text: 'Bewegung beendet',
        user: ''
    });
    console.debug("telegram['']: " + 'Bewegung beendet');
    Virt_Schalter_Teichbewegung = false;
  }
});

